  "scripts": {
    "build": "babel-node ./node_modules/webpack/bin/webpack",
    "manifest": "fbt-manifest --src src",
    "collect-fbts": "fbt-collect --pretty --manifest < .src_manifest.json > .source_strings.json",
    "translate-fbts": "fbt-translate --translations translations/*.json --jenkins > src/translatedFbts.json",
    "translate-fbts-single-file": "fbt-translate --jenkins --stdin < translation_input.json > src/translatedFbts.json",
    "test-collect-fbts": "collect-fbt --plugins @babel/plugin-syntax-flow --pretty --manifest < .src_manifest.json > .test_source_strings.json",
    "test-translate-fbts-into-output-dir": "fbt-translate --translations translations/*.json --jenkins --output-dir src/translatedFbts",
    "clean-fbts": "rm .enum_manifest.json .src_manifest.json .source_strings.json src/translatedFbts.json 2&> /dev/null || exit 0",
    "start": "babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --open",
    "test": "babel-node ./node_modules/jest/bin/jest --config ./jest.config.json"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-flow": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-syntax-numeric-separator": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-optional-catch-binding": "^7.2.0",
    "@babel/plugin-syntax-optional-chaining": "^7.2.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.4",
    "classnames": "2.2.6",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "flow-bin": "0.87.0",
    "flow-remove-types": "^1.2.3",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "invariant": "^2.2.4",
    "jest": "^24.8.0",
    "jest-docblock": "^22.2.0",
    "normalize.css": "8.0.1",
    "raf": "3.4.1",
    "react-test-renderer": "16.6.3",
    "remove-flow-types-loader": "1.1.0",
    "shelljs": "^0.8.3",
    "style-loader": "0.23.1",
    "webpack": "^4.26.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "^3.1.11",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-syntax-numeric-separator": "^7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "babel-plugin-fbt": ">= 0.11.0",
    "babel-plugin-fbt-runtime": "^0.9.8",
    "babel-plugin-minify-replace": "0.5.0",
    "fbjs": "1.0.0",
